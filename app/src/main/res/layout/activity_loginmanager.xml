<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginManagerActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/loginmanager_recylcer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/loginmanager_recylcer"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/simpleMargin"
        android:layout_marginTop="@dimen/simpleMargin"
        android:clickable="true"
        app:srcCompat="@drawable/ic_add_24"
        android:focusable="true"
        android:onClick="onAdd"/>


</RelativeLayout>